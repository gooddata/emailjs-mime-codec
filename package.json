{
  "name": "@gooddata/emailjs-mime-codec-new",
  "description": "Encode and decode quoted printable and base64 strings",
  "version": "3.0.2",
  "main": "dist/mimecodec",
  "homepage": "https://github.com/gooddata/emailjs-mime-codec",
  "repository": {
    "type": "git",
    "url": "git://github.com/gooddata/emailjs-mime-codec.git"
  },
  "license": "MIT",
  "keywords": [
    "MIME",
    "Base64",
    "Quoted-Printable"
  ],
  "author": "Andris Reinman <andris@kreata.ee>",
  "scripts": {
    "build": "./scripts/build.sh",
    "lint": "$(npm root)/.bin/standard",
    "test": "npm run lint && npm run unit",
    "unit": "$(npm root)/.bin/mocha './src/*-unit.js' --reporter spec --require babel-register testutils.js"
  },
  "dependencies": {
    "emailjs-base64": "^1.1.4",
    "ramda": "0.31.3",
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "pre-commit": "^1.2.2",
    "standard": "^12.0.1"
  },
  "files": [
    "dist",
    "scripts",
    "src",
    "README.md",
    "LICENSE"
  ],
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeAll",
      "afterAll",
      "after",
      "expect"
    ],
    "ignore": [
      "dist"
    ]
  }
}
